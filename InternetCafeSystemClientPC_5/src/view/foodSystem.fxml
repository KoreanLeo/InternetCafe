            <Bloom threshold="0.43">
               <input>
                  <Shadow />
               </input>
            </Bloom>
         </effect>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <ImageView id="foodSystem2" fx:id="showOrderstage2" fitHeight="129.0" fitWidth="298.0" layoutX="34.0" layoutY="32.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/food.png" />
         </image>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
      <Label id="foodSystemOn" layoutX="323.0" layoutY="7.0" text="Go!" textFill="#d11111">
         <font>
            <Font name="Showcard Gothic" size="18.0" />
         </font>
      </Label>
